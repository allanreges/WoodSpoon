{
  "env": {
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-unused-vars": "off",
    "no-unused-expressions": "off",
    "import/prefer-default-export": "off",
    "import/no-default-export": "error"
  }
}
